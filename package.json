{
  "name": "kysely",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "tsc && tsc-alias",
    "start": "node ./dist/index.js",
    "dev": "nodemon -e ts --exec ts-node -r tsconfig-paths/register ./src/index.ts",
    "kysely": "kysely",
    "kysely:generate": "kysely-codegen --out-file=/app/src/types/db.d.ts"
  },
  "dependencies": {
    "dotenv": "^16.4.5",
    "express": "^4.20.0",
    "kysely": "^0.27.4",
    "pg": "^8.12.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^22.4.1",
    "@types/pg": "^8.11.6",
    "kysely-codegen": "^0.16.3",
    "kysely-ctl": "^0.9.0",
    "nodemon": "^3.1.4",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.10",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.5.4"
  }
}
